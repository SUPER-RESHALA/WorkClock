<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <View
        android:id="@+id/view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:id="@+id/timeMain"
        android:layout_width="895dp"
        android:layout_height="249dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginTop="129dp"
        android:layout_marginEnd="164dp"
        android:layout_marginBottom="422dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/dateMain"
        android:layout_width="691dp"
        android:layout_height="117dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="280dp"
        android:layout_marginBottom="285dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/instructor"
        android:layout_width="968dp"
        android:layout_height="70dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="101dp"
        android:layout_marginBottom="151dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/recordedDate"
        android:layout_width="503dp"
        android:layout_height="77dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="366dp"
        android:layout_marginBottom="431dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/recordedTime"
        android:layout_width="459dp"
        android:layout_height="109dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="411dp"
        android:layout_marginBottom="516dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/textReg"
        android:layout_width="601dp"
        android:layout_height="78dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="303dp"
        android:layout_marginBottom="342dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/textUnreg"
        android:layout_width="817dp"
        android:layout_height="213dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="173dp"
        android:layout_marginBottom="70dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/code"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="1118dp"
        android:layout_marginBottom="438dp"
        android:text="TextView" />

    <EditText
        android:id="@+id/cardNumRFID"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="1dp"
        android:layout_marginTop="-1dp"
        android:layout_marginEnd="1138dp"
        android:layout_marginBottom="700dp"
        android:ems="10"
        android:inputType="number"


        tools:visibility="gone" />


    <ImageView
        android:id="@+id/imageView"
        android:layout_width="189dp"
        android:layout_height="138dp"
        android:layout_alignBottom="@+id/cardNumRFID"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="-2dp"
        android:layout_marginTop="-7dp"
        android:layout_marginEnd="1092dp"
        android:layout_marginBottom="-31dp"
        app:srcCompat="@android:drawable/ic_notification_overlay" />

</RelativeLayout>